{
    "compilerOptions": {
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strict": true,
        "target": "ES3",
        "module": "commonjs",
        "types": ["ImageLightbox", "JQuery"],
        "typeRoots": [
            "src/@types"
        ],
        "lib": [
            "dom",
            "es2016"
        ]
    },
    "files": [
        "src/index.ts"
    ],
    "outDir": "dist2"
}
